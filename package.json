{
  "name": "@fullstackcraftllc/codevideo-backend-engine",
  "version": "1.0.3",
  "description": "Create shockingly realistic automated software videos! The backend / CLI tool from CodeVideo to create videos.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js"
  },
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "build-typescript": "tsc",
    "start": "tsc && node ./dist/make-video-legacy.js",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf ./dist/* && rm -rf ./audio/*.mp3 && rm -rf ./video/*.mp4",
    "make-video": "tsc && node ./dist/make-video.js",
    "scripts-char-count": "tsc && node ./dist/scripts/scripts-char-count.js",
    "scripts-health-check": "tsc && node ./dist/scripts/scripts-health-check.js",
    "code-health-check": "tsc && node ./dist/scripts/code-health-check.js",
    "visual-studio-code-driver": "tsc && node ./dist/visual-studio-code-driver.js"
  },
  "keywords": [
    "codevideo",
    "typescript",
    "automation",
    "video",
    "ffmpeg",
    "puppeteer",
    "openai",
    "elevenlabs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fullstackcraftllc/codevideo-types": "^1.2.1",
    "@fullstackcraftllc/syntax-spy": "^1.0.4",
    "@fullstackcraftllc/virtual-code-block": "^1.3.4",
    "isomorphic-fetch": "^3.0.0",
    "jimp": "^0.22.10",
    "monaco-editor": "^0.45.0",
    "openai": "^4.24.7",
    "puppeteer": "^21.7.0",
    "puppeteer-screen-recorder": "^2.1.2",
    "robotjs": "^0.6.0",
    "say": "^0.16.0",
    "sound-play": "^1.1.0",
    "tesseract.js": "^5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/jest": "^29.5.12",
    "@types/play-sound": "^1.1.2",
    "@types/sound-play": "^1.1.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-esm-transformer": "^1.0.0",
    "patch-package": "^8.0.0",
    "rollup": "^4.13.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.3.3"
  }
}
